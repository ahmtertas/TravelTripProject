@model List<TravelTripProject.Models.Classes.Blog>
<div class="padding1">
    <h4>En Beğendiğim Yerler</h4>

    <div class="box-left">
        @foreach (var item in Model)
        {
            <div class="bot-img">
                <img src="@item.BlogImage" alt="" style="width:120px; height:60px;"/>
            </div>
            <div class="bot-desc">
                <p>@item.BlogOzet.Substring(0, 60)</p>
            </div>
            <div class="clear"></div>
        }
    </div>
    @Html.Action("HomeLastPartialRight", "Default")
</div>

